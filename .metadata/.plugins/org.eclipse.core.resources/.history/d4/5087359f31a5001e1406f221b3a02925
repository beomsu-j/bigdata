package 산술연산자;

public class Ex04증강연산자 {

	public static void main(String[] args) {

		int num = 4;
		// 증감연산자 -->
		System.out.println(++num);
		System.out.println(num);
		System.out.println(num++);
		System.out.println(num);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}

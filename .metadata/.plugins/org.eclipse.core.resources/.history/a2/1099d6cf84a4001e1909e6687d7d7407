package 변수및자료형;

public class Ex01입출력 {

	public static void main(String[] args) {
        // 주석 : 코드에 영향을 끼치지 않는 일종의 메모 역할을 한다
		// 단축키
		// 1) 글자 크기 : ctrl + , ctrl _
		// 2) 한 줄 삭제 : ctrl d
		// 3) 저장 : ctrl s
		
		// 1. 출력문
		// ---> () 안쪽에 출력하고 싶은 구문 작성
		System.out.println("Hello world!!!");
		System.out.println("조범수");
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
